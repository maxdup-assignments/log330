{
  "name": "referenceIt",
  "version": "1.0",
  "description": "Bookmark managing extension",
  "manifest_version": 2,
  "options_page": "options.html",
  "background": {
      "persistent": false,
      "scripts": ["js/background.js"]
  },  
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "<all_urls>",
    "http://*/*", "https://*/*", "background"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}