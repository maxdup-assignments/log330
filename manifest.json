{
  "name": "referenceIt",
  "version": "1.0",
  "description": "Bookmark managing extension",
  "manifest_version": 2,
  "options_page": "options.html",
  "background": {
      "persistent": false,
      "scripts": ["js/background.js"]
  },  
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "<all_urls>",
    "http://*/*", "https://*/*", "background"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
      "matches": ["<all_urls>","http://*/*", "https://*/*","*://*/*"],
      "js": ["js/jquery-2.1.4.min.js", "js/dom.js"],
      "run_at": "document_end"
  }]
}